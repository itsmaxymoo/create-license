#!/usr/bin/php
<?php namespace {class PHPCanner{const NAME='create-license';const DESCRIPTION='Easily create license files.';const VERSION='1.0';const LICENSE='MIT License';const CONTRIBUTORS='https://github.com/itsmaxymoo';const PCAN_VERSION='3.0';public static $files=['php::CreateLicense/Program' => '','licenses.json' => '','templates/agpl3.txt' => '','templates/apl2.txt' => '','templates/gpl3.txt' => '','templates/lgpl3.txt' => '','templates/mit.txt' => '','templates/mpl2.txt' => '','templates/unlicense.txt' => ''];public static function end($text = ''){self::log($text);die();}public static function log($text = ''){if(!defined('PHP_WINDOWS_VERSION_MAJOR')){$term_width = intval(exec('tput cols'));if($term_width === 0){$term_width = 80;}$text = wordwrap($text, $term_width, PHP_EOL, true);}echo $text . PHP_EOL;}public static function get_file($file_name){$file_name = str_replace('\\', '/', $file_name);return base64_decode(self::$files[$file_name]);}public static function put_file($internal_path, $file_data){$internal_path = str_replace('\\', '/', $internal_path);self::$files[$internal_path] = base64_encode($file_data);}public static function get_exec_name(){return __FILE__;}}?><?php if(PHP_SAPI !== 'cli'){\PHPCanner::end('create-license must be ran from the command line.');}else{if(isset(getopt('', ['pcan-version'])['pcan-version'])){\PHPCanner::log(\PHPCanner::NAME . ' version ' . \PHPCanner::VERSION . PHP_EOL);\PHPCanner::end('Built by PHPCanner version ' . \PHPCanner::PCAN_VERSION);}spl_autoload_register(function($class){$class = str_replace('\\', '/', $class);eval(\PHPCanner::get_file('php::' . $class));});CreateLicense\Program::Main($argv);\PHPCanner::end();}}?>